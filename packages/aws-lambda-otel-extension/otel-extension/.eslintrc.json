{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": false,
    "amd": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "no-octal": 0,
    "no-case-declarations": 0
  }
}